<h1>Edit a new Landmark</h1>

<form action="/landmarks/<%=@landmark.id%>" method="POST">
  <input id="hidden" type="hidden" name="_method" value="patch">
  <label for="landmark_name">Name:</label>
  <input id= "name" type="text" name="landmark[name]" value=<%=@landmark.name%>>
  <label for="landmark_year_completed">Year Completed:</label>
  <input id= "year_completed" type="text" name="landmark[year_completed]"
  value=<%=@landmark.year_completed%>>

  <br></br>

  <input type="submit" value="Edit Landmark">
</form>
