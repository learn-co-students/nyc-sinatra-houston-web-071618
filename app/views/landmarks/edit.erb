<h1>Edit Landmark</h1>

<form action="/landmarks/<%=@landmark.id %>" method="POST">
  <input id= "hidden" type="hidden" name="_method" value="PATCH">
  <p>Name: <input id="name" name="landmark[name]" value="<%=@landmark.name%>"></p>
  <p>Year Completed: <input id="year_completed" name="landmark[year_completed]" value="<%=@landmark.year_completed%>"></p>

  <br></br>

  <input type="submit" value="Edit Landmark">

</form>
